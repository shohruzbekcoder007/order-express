{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "**/*.js": {
            "includeFiles": [
                "prisma/schema.prisma",
                "node_modules/.prisma/client/**/*"
            ]
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}